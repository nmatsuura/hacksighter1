
<%= form_for @sighting, html: {multipart: true} do |f| %>

	
	<div><%= f.label :topic_id %> <br />
		<%= f.select :topic_id, ['1', '2', '3', '4'] %>
	</div>

	<div>
		<%= f.label :location %><br />
		<%= f.text_field :location %><br />	
	</div>


	<%= f.fields_for :photo, @photo do |builder| %>
		<div>
		<%= builder.label :image %><br />
		<%= builder.file_field :image %><br />	
	</div>
	
<% end %>




	
	<%= f.submit 'Submit' %>

<% end %>